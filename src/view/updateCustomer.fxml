<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="463.0" prefWidth="749.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.updateCustomerController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="40.0" prefWidth="144.0" text="Update Customer">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="updateCustomerId" disable="true" editable="false" layoutX="62.0" layoutY="81.0" promptText="Auto - gen Customer ID" />
      <TextField fx:id="customerNameTf" layoutX="62.0" layoutY="123.0" promptText="Customer Name" />
      <TextField fx:id="addressTf" layoutX="62.0" layoutY="164.0" promptText="Address" />
      <TextField fx:id="postalCodeTf" layoutX="62.0" layoutY="207.0" promptText="Postal Code" />
      <Button layoutX="296.0" layoutY="289.0" mnemonicParsing="false" onAction="#saveCustomer" text="Save" />
      <TextField fx:id="phoneTf" layoutX="62.0" layoutY="246.0" promptText="Phone" />
      <ComboBox fx:id="countryCombo" layoutX="62.0" layoutY="289.0" onAction="#initDivisions" prefWidth="150.0" />
      <ComboBox fx:id="divisionCombo" layoutX="62.0" layoutY="335.0" prefWidth="150.0" />
   </children>
</AnchorPane>
